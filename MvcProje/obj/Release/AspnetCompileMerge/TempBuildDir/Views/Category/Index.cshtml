@model IEnumerable<EntityLayer.Concrete.Category>
@{
    Layout = null;
}

<h2>Index</h2>

<table>
    <tr>
        <th>ID</th>
        <th>Category Name</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.CategoryID</td>
            <td>@item.CategoryName</td>
        </tr>
    }
</table>
